@model IEnumerable<RentalKendaraan_20180140052.Models.Peminjaman>

@{
    ViewData["Title"] = "Index";
}

<h2>Tambah Data Peminjaman</h2>

<h4>Tambah Data Baru</h4>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.TglPeminjaman)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Biaya)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdCostumerNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdJaminanNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdKendaraanNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdPeminjamanNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TglPeminjaman)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Biaya)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdCostumerNavigation.IdCustomer)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdJaminanNavigation.IdJaminan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdKendaraanNavigation.IdKendaraan)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdPeminjamanNavigation.IdPeminjaman)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdPeminjaman"><i class="fas fa-pen-alt" style="color:blue"></i></a> |
                <a asp-action="Details" asp-route-id="@item.IdPeminjaman"><i class="fas fa-list" style="color:blue"></i></a> |
                <a asp-action="Delete" asp-route-id="@item.IdPeminjaman"><i class="fas fa-trash-alt" style="color:blue"></i></a>
            </td>
        </tr>
}
    </tbody>
</table>
